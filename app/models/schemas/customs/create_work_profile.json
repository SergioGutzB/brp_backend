{
  "title": "Employee work profile creation parameters",
  "type": "object",
  "properties": {
    "work_city": {
      "type": "string"
    },
    "work_state": {
      "type": "string"
    },
    "seniority_company": {
      "type": "string",
      "enum": ["less_than_a_year", "more_than_a_year"]
    },
    "position_held": {
      "type": "string"
    },
    "seniority_office": {
      "type": "string",
      "enum": [
        "less_than_1_year",
        "1_to_2",
        "3_to_5",
        "6_to_10",
        "11_or_more_years"
      ]
    },
    "work_area": {
      "type": "string",
      "enum": ["production", "administrative"]
    },
    "contract_type": {
      "type": "string",
      "enum": [
        "temporary_less_than_1_year",
        "temporary_1_year_or_more",
        "indefinite_term",
        "cooperative",
        "service_provision",
        "i_dont_know"
      ]
    },
    "working_hours_per_day": {
      "type": "number"
    },
    "salary_type": {
      "type": "string",
      "enum": ["fixed", "fixed_and_variable", "variable"]
    },
    "occupation": {
      "type": "string"
    }
  },
  "required": [
    "work_city",
    "work_state",
    "seniority_company",
    "position_held",
    "seniority_office",
    "work_area",
    "contract_type",
    "working_hours_per_day",
    "salary_type",
    "occupation"
  ]
}
