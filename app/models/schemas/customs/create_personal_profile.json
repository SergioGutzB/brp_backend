{
  "title": "Employee personal profile creation parameters",
  "type": "object",
  "properties": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "document_id": {
      "type": "string"
    },
    "gender": {
      "type": "string",
      "enum": ["male", "female"]
    },
    "birthdate": {
      "type": "integer"
    },
    "civil_status": {
      "type": "string",
      "enum": [
        "single",
        "married",
        "common_law",
        "divorced_or_separated",
        "widowed",
        "priest_or_nun"
      ]
    },
    "level_education": {
      "type": "string",
      "enum": [
        "none",
        "incomplete_primary",
        "complete_primary",
        "incomplete_secondary",
        "complete_secondary",
        "incomplete_technical_or_technological",
        "complete_technical_or_technological",
        "incomplete_professional",
        "complete_professional",
        "military_or_police_career",
        "incomplete_postgraduate",
        "complete_postgraduate"
      ]
    },
    "socioeconomic_status": {
      "type": "string",
      "enum": ["1", "2", "3", "4", "5", "6", "farm", "i_dont_know"]
    },
    "living_place": {
      "type": "string",
      "enum": ["own", "rented", "family"]
    },
    "people_dependents": {
      "type": "integer"
    },
    "residence_state": {
      "type": "string"
    },
    "residence_city": {
      "type": "string"
    }
  },
  "required": [
    "first_name",
    "last_name",
    "document_id",
    "gender",
    "birthdate",
    "civil_status",
    "level_education",
    "socioeconomic_status",
    "living_place",
    "people_dependents",
    "residence_state",
    "residence_city"
  ]
}
